<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bottom_nav_graph"
    app:startDestination="@id/analyzesFragment">

    <fragment
        android:id="@+id/analyzesFragment"
        android:name="com.example.smartlab.view.fragments.AnalyzesFragment"
        android:label="AnalyzesFragment"
        tools:layout="@layout/fragment_analyzes">
        <action
            android:id="@+id/action_analyzesFragment_to_cartFragment"
            app:destination="@id/cartFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/resultsFragment"
        android:name="com.example.smartlab.view.fragments.ResultsFragment"
        android:label="ResultsFragment"
        tools:layout="@layout/fragment_results" />
    <fragment
        android:id="@+id/supportFragment"
        android:name="com.example.smartlab.view.fragments.SupportFragment"
        android:label="SupportFragment"
        tools:layout="@layout/fragment_support" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.smartlab.view.fragments.ProfileFragment"
        android:label="ProfileFragment"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/cartFragment"
        android:name="com.example.smartlab.view.fragments.CartFragment"
        android:label="CartFragment"
        tools:layout="@layout/fragment_cart">
        <action
            android:id="@+id/action_cartFragment_to_orderFragment"
            app:destination="@id/orderFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/orderFragment"
        android:name="com.example.smartlab.view.fragments.OrderFragment"
        android:label="OrderFragment"
        tools:layout="@layout/fragment_order">
        <action
            android:id="@+id/action_orderFragment_to_waitingFragment"
            app:destination="@id/waitingFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/waitingFragment"
        android:name="com.example.smartlab.view.fragments.WaitingFragment"
        android:label="WaitingFragment"
        tools:layout="@layout/fragment_waiting" />

</navigation>